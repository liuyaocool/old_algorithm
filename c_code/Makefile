
all: 
	make -C ./src/

clean:
	cd src && make clean

main:
	@make main -C ./src/
	@echo -------------------------------------------------------
	@echo ------------------------ start ------------------------
	@echo -------------------------------------------------------
	@./bin/main

%:
	@clear
	@make main_$* -C ./src/
	@echo -------------------------------------------------------
	@echo ------------------------ start ------------------------
	@echo -------------------------------------------------------
	@./bin/main_$*