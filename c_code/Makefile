
all: 
	make -C ./src/

clean:
	cd src && make clean

main:
	@make main -C ./src/
	@echo -------------------------------------------------------
	@echo ------------------------ start ------------------------
	@echo -------------------------------------------------------
	@./bin/main

t%:
	@clear
	@make main $@ -C ./src/
	@echo -------------------------------------------------------
	@echo ------------------------ start ------------------------
	@echo -------------------------------------------------------
	@./bin/main_$*_*

r%:
	@./bin/main_$*_*
